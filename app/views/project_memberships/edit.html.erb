<h1>Editing project_membership</h1>

<% form_for(@project_membership) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :joined %><br />
    <%= f.date_select :joined %>
  </p>
  <p>
    <%= f.label :leaved %><br />
    <%= f.date_select :leaved %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :project %><br />
    <%= f.text_field :project %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @project_membership %> |
<%= link_to 'Back', project_memberships_path %>